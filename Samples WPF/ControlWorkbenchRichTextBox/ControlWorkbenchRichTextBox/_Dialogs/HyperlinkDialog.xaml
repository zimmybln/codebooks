<Window x:Class="ControlWorkbenchRichTextBox.HyperlinkDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="HyperlinkDialog" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" Height="185" Width="458" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid>
        <TextBox Height="29" HorizontalAlignment="Left" Margin="123,59,0,0" Name="txtHyperlinkTarget" VerticalAlignment="Top" Width="297" />
        <Label Content="Anzeigetext" Height="29" HorizontalAlignment="Left" Margin="12,14,0,0" Name="label1" VerticalAlignment="Top" Width="105" />
        <Button Content="Cancel" HorizontalAlignment="Left" Name="cmdCancel" VerticalAlignment="Top" Style="{StaticResource ResourceKey=DialogButton}" Margin="320,102,0,0" Click="OnDialogResult" />
        <Button Content="OK" HorizontalAlignment="Left" Margin="214,102,0,0" Name="cmdOK" Style="{StaticResource DialogButton}" VerticalAlignment="Top" Click="OnDialogResult" />
        <TextBox Height="29" HorizontalAlignment="Left" Margin="123,14,0,0" Name="txtHyperlinkText" VerticalAlignment="Top" Width="297" />
        <Label Content="Verknüpfung" Height="29" HorizontalAlignment="Left" Margin="12,57,0,0" Name="label2" VerticalAlignment="Top" Width="105" />
    </Grid>
</Window>
