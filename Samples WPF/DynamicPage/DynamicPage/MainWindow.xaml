<Window x:Class="DynamicPage.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50*" />
            <RowDefinition Height="50*" />
            <RowDefinition Height="35" />
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0" Margin="5" Name="txtSource"/>
        <Frame Grid.Row="1" Margin="5" Name="frame"/>
        <Button Grid.Row="2" Height="28" Width="80" HorizontalAlignment="Right" VerticalAlignment="Center" Content="Ausführen" Click="OnClick" Margin="0,0,10,0"/>
    </Grid>
</Window>
